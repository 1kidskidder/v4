--> Silent Aim | Global Table <--
getgenv().Silent = {
    Settings = {
        Toggled = true,
        AimPart = "Head", --> HumanoidRootPart, UpperTorso, LowerTorso, Head <-- Main Parts
        HitChance = 1000,

        Prediction = {
            Enabled = true,
            Horizontal = 0.1433, --> X, Z
            Vertical = 0.1433, --> Y
        },
       
        Circle = {
            Visible = true,
            Color = Color3.fromRGB(0, 0, 0),
            Transparency = 2,
            Thickness = 2,
            NumSides = 1000,
            Radius = 60, --> Change Circle Size Here
            Filled = false,
        },
    },
}

